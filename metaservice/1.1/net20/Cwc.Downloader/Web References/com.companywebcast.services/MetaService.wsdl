              <xsd:appinfo />
              <xsd:documentation>Gets the tag collection to add or remove tags to/from.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Title" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the Webcast title</xsd:documentation>
