            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ScheduledStart" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the planned start date/time</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Slides" nillable="true" type="tns:Slides">
