          <xsd:appinfo />
          <xsd:documentation>Attachment info.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="ContentType" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the attachment contenttype indicating the data format of the attachment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Description" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the attachment description.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Location" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the location where the attachment can be downloaded.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Size" nillable="true" type="xsd:long">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the attachment size in bytes.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Attachment" nillable="true" type="tns:Attachment" />
      <xsd:complexType name="Languages">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Language code collection.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="string" nillable="true" type="xsd:string" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Languages" nillable="true" type="tns:Languages" />
      <xsd:complexType name="Location">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Describes the location of the webcast event. This can be in postal and or geographical format.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" name="Address" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the first address line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="City" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the second address line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Coordinates" nillable="true" type="tns:Coordinates">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the geographical location.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CountryCode" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the two letter iso country code.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PostalCode" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the postal code.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="State" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the state.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="TimeZoneName" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets time-zone identifier based on ISO zone tab name. See http://en.wikipedia.org/wiki/Zone.tab</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Location" nillable="true" type="tns:Location" />
      <xsd:complexType name="Coordinates">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Describes the geographical location.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="Latitude" type="xsd:double">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the latitude.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Longitude" type="xsd:double">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the longitude.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Coordinates" nillable="true" type="tns:Coordinates" />
      <xsd:complexType name="MediaStreams">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Media stream collection.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="MediaStream" nillable="true" type="tns:MediaStream" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="MediaStreams" nillable="true" type="tns:MediaStreams" />
      <xsd:complexType name="MediaStream">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Webcast audio/video stream information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent mixed="false">
          <xsd:extension base="tns:Attachment">
            <xsd:sequence>
              <xsd:element name="Bitrate" type="xsd:unsignedInt">
                <xsd:annotation>
                  <xsd:appinfo />
                  <xsd:documentation>Gets or sets the (maximum) bitrate of this data file/stream.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="Dimensions" nillable="true" type="tns:Dimensions">
                <xsd:annotation>
                  <xsd:appinfo />
                  <xsd:documentation>Gets or sets the dimensions of this file/stream when type is Video.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="End" nillable="true" type="xsd:dateTime">
                <xsd:annotation>
                  <xsd:appinfo />
                  <xsd:documentation>Gets or sets the actual utc end of the stream.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="Gaps" nillable="true" type="tns:Gaps">
                <xsd:annotation>
                  <xsd:appinfo />
                  <xsd:documentation>Gets or sets a collection of gaps that mark missing parts within this multimedia stream.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="Id" nillable="true" type="xsd:string">
                <xsd:annotation>
                  <xsd:appinfo />
                  <xsd:documentation>Gets or sets the unieque identifier.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="Start" nillable="true" type="xsd:dateTime">
                <xsd:annotation>
                  <xsd:appinfo />
                  <xsd:documentation>Gets or sets the actual utc start of the stream.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="Type" type="tns:MediaStream.StreamType">
                <xsd:annotation>
                  <xsd:appinfo />
                  <xsd:documentation>Gets or sets the stream type referring to either an audio or video stream.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:element name="MediaStream" nillable="true" type="tns:MediaStream" />
      <xsd:complexType name="Dimensions">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Dimensions specified in whole units.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="Height" type="xsd:int">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the height.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Width" type="xsd:int">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the width.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Dimensions" nillable="true" type="tns:Dimensions" />
      <xsd:complexType name="Gaps">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Multimedia gaps collection.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="Gap" nillable="true" type="tns:Gap" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Gaps" nillable="true" type="tns:Gaps" />
      <xsd:complexType name="Gap">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Gap describes a location within the current stream where an interruption occured in the live feed. All events in our system are recorded with "live" time. These times should be corrected during playback if the events will be used to display additional information during playback.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="Correction" type="xsd:double">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the length in seconds of the interruption / gap.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="End" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the end in UTC when the actual live transmission got resumed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Offset" type="xsd:double">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the offset in seconds in the current stream where there is a missing</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Start" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the start in UTC when the actual live transmission got interrupted.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Gap" nillable="true" type="tns:Gap" />
      <xsd:simpleType name="MediaStream.StreamType">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Stream type data identifier.</xsd:documentation>
        </xsd:annotation>
        <xsd:list>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:enumeration value="Video" />
              <xsd:enumeration value="Audio" />
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:list>
      </xsd:simpleType>
      <xsd:element name="MediaStream.StreamType" nillable="true" type="tns:MediaStream.StreamType" />
      <xsd:complexType name="Slides">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Slide collection.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="Slide" nillable="true" type="tns:Slide" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Slides" nillable="true" type="tns:Slides" />
