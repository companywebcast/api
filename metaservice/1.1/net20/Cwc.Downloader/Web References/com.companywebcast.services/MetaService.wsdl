              <xsd:documentation>Gets or sets the webcast description.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Id" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the unieque identifier.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Language" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the webcast language code.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Languages" nillable="true" type="tns:Languages">
