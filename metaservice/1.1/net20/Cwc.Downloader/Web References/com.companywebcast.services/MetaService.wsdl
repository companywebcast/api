              <xsd:appinfo />
              <xsd:documentation>Gets or sets the possible slide source files containing just one slide.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Events" nillable="true" type="tns:Events">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the moments this slide should be presented in the webcast.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Id" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the unique identifier.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="RawText" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the text that is shown in the slide for indexing / search purposes.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Reference" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the custom external reference.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Slide" nillable="true" type="tns:Slide" />
      <xsd:complexType name="Speakers">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Speaker collection.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="Speaker" nillable="true" type="tns:Speaker" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Speakers" nillable="true" type="tns:Speakers" />
      <xsd:complexType name="Speaker">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Speaker information.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" name="Category" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
