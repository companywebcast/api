        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Slide information.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="Attachments" nillable="true" type="tns:Attachments">
