          </xsd:element>
          <xsd:element name="Topics" nillable="true" type="tns:Topics">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the webcast topics that are addressed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Webcast" nillable="true" type="tns:Webcast" />
      <xsd:complexType name="Annotations">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Annotation collection.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="Annotation" nillable="true" type="tns:Annotation" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Annotations" nillable="true" type="tns:Annotations" />
