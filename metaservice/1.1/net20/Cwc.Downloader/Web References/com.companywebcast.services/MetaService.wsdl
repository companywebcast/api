          <xsd:appinfo />
          <xsd:documentation>Webcast event information.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="End" nillable="true" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the time the event actually ended.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Start" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the time the event actually took place.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Event" nillable="true" type="tns:Event" />
      <xsd:complexType name="Attachments">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Attachment collection.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
