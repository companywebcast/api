              <xsd:appinfo />
              <xsd:documentation>Gets or sets the moment in the webcast to where this comments refers to.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Text" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo />
              <xsd:documentation>Gets or sets the comment description.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Annotation" nillable="true" type="tns:Annotation" />
      <xsd:complexType name="Events">
        <xsd:annotation>
          <xsd:appinfo />
          <xsd:documentation>Event collection.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="Event" nillable="true" type="tns:Event" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Events" nillable="true" type="tns:Events" />
      <xsd:complexType name="Event">
